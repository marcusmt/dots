layout {
    gaps 12
    center-focused-column "never"

    preset-column-widths {
        proportion 0.33333
        proportion 0.5
        proportion 0.66667
    }

    focus-ring {
        width 2
        active-color "#aaaaaa"
        inactive-color "#54546D"
    }

    struts {
        left 1
        right 1
        top 1
        bottom 1
    }
}

animations {
    off
}

